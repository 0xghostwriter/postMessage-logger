{
  "name": "postMessage logger",
  "description": "Simple postMessage logger",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "all_frames": true,
      "js": [
        "content_script.js"
      ]
    }
  ],
  "icons": {
    "128": "icon/128x128.png"
  },
  "manifest_version": 2
}